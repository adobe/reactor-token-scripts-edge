{
  "name": "@adobe/reactor-token-scripts-edge",
  "version": "1.1.0",
  "description": "Node binary used in transforming the edge container to valid JS.",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint 'src/**'",
    "debug:findTokens": "node cli.js findTokens --filePath ./test/fixtures/findTokens/container.js --onlyUndefined",
    "debug:sanitize": "node cli.js sanitize --filePath ./test/fixtures/sanitize/container.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe/reactor-token-scripts-edge.git"
  },
  "author": {
    "name": "Adobe Systems",
    "url": "http://adobe.com",
    "email": "reactor@adobe.com"
  },
  "license": "Apache-2.0",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint -- --fix"
    ]
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-runner-eslint": "^0.10.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.9",
    "yargs": "^16.1.1"
  },
  "bin": "cli.js"
}
